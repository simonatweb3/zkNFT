{
  "name": "@pomp-eth/contracts",
  "version": "0.0.1",
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "lint:sol": "solhint --config ./.solhint.json  \"contracts/**/*.sol\"",
    "lint:solfix": "solhint --config ./.solhint.json  \"contracts/**/*.sol\" --fix",
    "lint:ts": "eslint .",
    "lint": "yarn lint:ts; yarn lint:sol",
    "build": "yarn lint:sol && hardhat compile",
    "test": "yarn build && hardhat test"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.2",
    "circomlibjs": "^0.1.7"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.1",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@semaphore-protocol/contracts": "^3.10.1",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^8.0.0",
    "@types/mocha": "^9.1.0",
    "chai": "^4.3.7",
    "eslint": "8.45.0",
    "ethers": "^6.6.4",
    "hardhat": "^2.16.1",
    "hardhat-gas-reporter": "^1.0.8",
    "solhint": "^3.4.1",
    "solidity-coverage": "^0.8.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.0"
  }
}
